
 Type ::=
             INTEGER  {:RESULT = "INTEGER";:}
            | BOOL       {:RESULT = "BOOL";:}
            | REAL       {:RESULT = "REAL";:}
            | STRING        {:RESULT = "STRING";:}
            | CHAR       {:RESULT = "CHAR";:}
            ;
    ParDecl ::= Type IdList
                	| OUT Type IdList;

